
<?xml version='1.0' ?>
<root org="panfeng" 
	chinese_name="医生排班系统" 
	english_name="Doctor Scheduling System">

	
    <hospital

        name="上和医院"
        address="毕升路22号"
        telphone="028-9123123"

    
    />
	
    <expense_type
        name="诊疗费|治疗费|检查费"
        helper_chars="zlf|zlf|jcf"
        status="正常|停用"
        hospital="$(hospital)"
    />

     <expense_item
        name="专家诊疗费|血常规|煎药费"
        price="$99.00"
        expense_type="$(expense_type)"
        hospital="$(hospital)"
    />

    <doctor
        name="魏松全"
        hospital="$(hospital)"
    />

    <department
        name="放射科"
        hospital="$(hospital)"

    />
    
    <doctor_assignment
        name="魏松全在内分泌科室上"
        doctor="$(doctor)"
        department="$(department)"
        
    />
    <doctor_schedule
        name="2019年3月11日魏松全在内分泌科坐班"
        
    
    />

</root>
 